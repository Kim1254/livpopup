	"text":
	{
		"TextTitle":
		{
			"font": "Bahnschrift",
			"text": "The Little Three Pigs",
			"size": 2,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text01":
		{
			"font": "Bahnschrift",
			"text": "In a forest,\nthere were three little pigs.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text02":
		{
			"font": "Bahnschrift",
			"text": "One day,\nthe mother pig told the little pigs.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text03":
		{
			"font": "Bahnschrift",
			"text": "Guys, why don't you build your own house?",
			"size": 1,
			"height": 0.1,
			"color": "FF85FF"
		},
		"Text04":
		{
			"font": "Bahnschrift",
			"text": "Yes, mother!",
			"size": 1,
			"height": 0.1,
			"color": "F361DC"
		},
		"Text05":
		{
			"font": "Bahnschrift",
			"text": "The three little pigs left home",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text06":
		{
			"font": "Bahnschrift",
			"text": "The first pig was lazy.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text07":
		{
			"font": "Bahnschrift",
			"text": "He built a house by stacking straws.",
			"size": 1,
			"height": 0.1,
			"color": "ffff00"
		},
		"Text08":
		{
			"font": "Bahnschrift",
			"text": "By the way, a wolf appeared.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text09":
		{
			"font": "Bahnschrift",
			"text": "What? It's a straw house!",
			"size": 1,
			"height": 0.1,
			"color": "5555ff"
		},
		"Text10":
		{
			"font": "Bahnschrift",
			"text": "The house was blown away\nby the wolf's breath!",
			"size": 1,
			"height": 0.1,
			"color": "ff3333"
		},
		"Text11":
		{
			"font": "Bahnschrift",
			"text": "The first pig ran away to his another brother.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text12":
		{
			"font": "Bahnschrift",
			"text": "The second pig was full of pride.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text13":
		{
			"font": "Bahnschrift",
			"text": "He built a log cabin.",
			"size": 1,
			"height": 0.1,
			"color": "ffff00"
		},
		"Text14":
		{
			"font": "Bahnschrift",
			"text": "One day, the wolf appeared.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text15":
		{
			"font": "Bahnschrift",
			"text": "A wood house?\nit's no match for my head!",
			"size": 1,
			"height": 0.1,
			"color": "5555ff"
		},
		"Text16":
		{
			"font": "Bahnschrift",
			"text": "The house was broken\nby the wolf's headbutt!",
			"size": 1,
			"height": 0.1,
			"color": "ff3333"
		},
		"Text17":
		{
			"font": "Bahnschrift",
			"text": "The first pig ran away to his another brother.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text18":
		{
			"font": "Bahnschrift",
			"text": "The third pig was wise.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text19":
		{
			"font": "Bahnschrift",
			"text": "He built a brick house.",
			"size": 1,
			"height": 0.1,
			"color": "ffff00"
		},
		"Text20":
		{
			"font": "Bahnschrift",
			"text": "One day, the wolf appeared.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text21":
		{
			"font": "Bahnschrift",
			"text": "A brick house?\nit's no match for my head!",
			"size": 1,
			"height": 0.1,
			"color": "5555ff"
		},
		"Text22":
		{
			"font": "Bahnschrift",
			"text": "The house was fine!\nOnly the wolf's head is hurted",
			"size": 1,
			"height": 0.1,
			"color": "ff3333"
		},
		"Text23":
		{
			"font": "Bahnschrift",
			"text": "The wolf ran away to the forest.",
			"size": 1,
			"height": 0.1,
			"color": "ffffff"
		},
		"Text24":
		{
			"font": "Bahnschrift",
			"text": "Thanks to the third pig's wise,\nThe three little pigs lived in\nthe brick house together.",
			"size": 1,
			"height": 0.1,
			"color": "aaaa55"
		}
	},
